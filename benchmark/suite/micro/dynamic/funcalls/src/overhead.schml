(letrec ([id : (Dyn -> Dyn) (lambda ((x : Dyn)) x)])
  (let ([id : Dyn id]
        [iters : Dyn (read-int)]
        [acc : (GRef Dyn) (gbox 0)])
    (letrec ([run-test : (Int Dyn -> Dyn)
              (lambda (i acc) acc)])
      (begin
        (timer-start)
        (repeat (i 0 iters) (gbox-set! acc (run-test i (gunbox acc))))
        (timer-stop)
        (timer-report)
        (gunbox acc)))))


