;;; TAK -- A vanilla version of the TAKeuchi function.

(define (tak [x : Int] [y : Int] [z : Int]) : Int
  (if (>= y x)
      z
      (tak (tak (- x 1) y z)
           (tak (- y 1) z x)
           (tak (- z 1) x y))))

(let ([x (read-int)])
  (let ([y (read-int)])
    (let ([z (read-int)])
      (tak x y z))))
