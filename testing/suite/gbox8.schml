(let ((mre : Dyn #f))
 (let ((dynref : (GRef Dyn) (gbox mre)))
  (let ((boolref : (GRef Bool) dynref)
        (intref  : (GRef Int)  dynref))
   (if (gunbox boolref)
       (gunbox dynref)
       (begin (gbox-set! intref 8)
              (if (= (gunbox intref) 8)
                  (gunbox intref)
                  (gunbox boolref)))))))