(letrec ([fib : (Int -> Int)
          (lambda (n) 
           (let ([last  (gbox 0)]
                 [llast (gbox 1)])
            (begin
             (repeat (n 0 n)
             (let ([tmp (gunbox llast)])
              (begin
               (gbox-set! llast (gunbox llast))
               (gbox-set! last  (+ (gunbox last) tmp)))))
             (gunbox last))))])
  (fib 10))
                           
                             